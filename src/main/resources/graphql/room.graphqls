enum RoomType {
  SINGLE
  DOUBLE
  SUITE
}

type Room {
  hotelId: String!
  type: RoomType!
  pricePerNight: Int!
  totalRooms: Int!
  availableRooms: Int!
  amenities: [String]
  images: [String]
}

input RoomInput {
  type: RoomType!
  pricePerNight: Int!
  totalRooms: Int!
  availableRooms: Int!
  amenities: [String]
  images: [String]
}

input ModifyRoomInput {
  type: RoomType
  pricePerNight: Int
  totalRooms: Int
  availableRooms: Int
  amenities: [String]
  images: [String]
}

